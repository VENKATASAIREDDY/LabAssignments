<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.cg.sl1.beans.Employee" id="employee">
		<property value="12345" name="employeeId"/>
		<property value="Venkata Sai" name="employeeName"/>
		<property value="40000.0" name="salary"/>
		<property name ="businessUnit" ref="sbu"/>
		<property value="22" name="age"/>
	</bean>
	
	
	
	<bean class="com.cg.sl1.beans.SBU" id="sbu">
		<property value="PES-BU" name="sbuID"/>
		<property value="Product Engineer Services" name="sbuName"/>
		<property value="Kiran Rao" name="sbuHead"/>
<!-- 		<property name="empList"></property> -->
	</bean>
	

</beans>